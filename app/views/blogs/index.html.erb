<p id="notice"><%= notice %></p>

<h1>Blogs</h1>


  <div>
    <tr>
     <div>
    <% @blogs.each do |blog| %>
      <div>
        <th>Title:</th><p><%= blog.title %></p>
         <th>Body:</th><p><%= blog.body %></p> 
        <p><%= link_to 'Show', blog %></p>
        <p><%= link_to 'Edit', edit_blog_path(blog) %></p>
        <p><%= link_to 'Destroy', blog, method: :delete, data: { confirm: 'Are you sure?' } %></p>
      </div>
    <% end %>
  </div>
      
     
      <th colspan="3"></th>
    </tr>
  </div>

 


