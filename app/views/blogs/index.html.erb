<p id="notice"><%= notice %></p>

<h1>Blogs</h1>


  <div>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </div>

  <div>
    <% @blogs.each do |blog| %>
      <div>
        <p><%= blog.title %></p>
        <p><%= blog.body %></p>
        <p><%= link_to 'Show', blog %></p>
        <p><%= link_to 'Edit', edit_blog_path(blog) %></p>
        <p><%= link_to 'Destroy', blog, method: :delete, data: { confirm: 'Are you sure?' } %></p>
      </div>
    <% end %>
  </div>


<br>

<%= link_to 'New Blog', new_blog_path %>
