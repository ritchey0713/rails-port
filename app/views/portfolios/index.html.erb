<!-- pass collection and partial keys when namespace of varibale is not the same as model name ie Portfolio != portfolio_items -->
<main role="main">
    <div class="album py-5 bg-light">
        <div class="container">

            <div class="row sortable">
                <%=  render partial: 'portfolio_item', collection: @portfolio_items %>
            </div>
        </div>
    </div>
</main>


 
  
